{
  "name": "@maverick/router",
  "description": "router for mav",
  "license": "GPL-2.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.4.1",
  "homepage": "https://mav.xyz",
  "keywords": [
    "maverick",
    "router"
  ],
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@maverick/contracts": "file:../maverick-v1",
    "@openzeppelin/contracts": "<4.8.0",
    "@tenderly/hardhat-tenderly": "^1.0.13",
    "base64-sol": "^1.0.1",
    "hardhat-tracer": "^1.0.0-alpha.6",
    "lodash": "^4.17.21",
    "prb-math": "^2.2.0",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "web3": "^1.4.0"
  },
  "devDependencies": {
    "@defi-wonderland/smock": "^2.0.7",
    "@nomiclabs/ethereumjs-vm": "^4.2.2",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/hardhat-upgrades": "^1.10.0",
    "@typechain/ethers-v5": "7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.16",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "bdd-lazy-var": "^2.6.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "coingecko-api-v3": "^0.0.13",
    "decimal.js": "^10.2.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.0",
    "hardhat": "^2.9.5",
    "hardhat-contract-sizer": "^2.1.1",
    "hardhat-deploy": "^0.9.0-next.0",
    "hardhat-gas-reporter": "^1.0.4",
    "mocha": "^9.1.3",
    "mocha-chai-jest-snapshot": "^1.1.1",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^5.1.2",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "compile": "hardhat compile",
    "build": "hardhat compile && hardhat typechain",
    "test": "hardhat test",
    "test:prettier": "prettier --write 'test/**/**/**/*.ts'",
    "test:parallel": "mocha --require hardhat/register --recursive --parallel --exit",
    "test:coverage": "hardhat coverage",
    "deploy": "hardhat deploy",
    "dev": "hardhat node --watch",
    "lint": "solhint -f table 'contracts/**/**/**/*.sol'",
    "prettier": "prettier --write 'contracts/**/**/**/*.sol'",
    "prettier-check-sol": "prettier --check 'contracts/**/**/**/*.sol'",
    "prettier-check-test": "prettier --check 'test/**/**/**/*.ts'",
    "prettier-check": "yarn prettier-check-sol && yarn prettier-check-test",
    "size": "yarn build && hardhat size-contracts"
  },
  "packageManager": "yarn@2.4.3"
}
